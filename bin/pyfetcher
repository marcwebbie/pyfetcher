#!/usr/bin/env python
# -*- coding: utf-8 -*-

import os
import sys

if __name__ == "__main__":
    # to test, from a virtualenv type
    # pip uninstall pyfetcher && python setup.py install && && python setup.py clean && pyfetcher

    # add pyfetcher to the path
    import pyfetcher
    sys.path.append(os.path.dirname(pyfetcher.__file__))

    pyfetcher.main()
